{
  "name": "baddbeatz",
  "compatibility_date": "2024-12-01",
  "main": "workers-site/index.js",
  "kv_namespaces": [
    {
      "binding": "RATE_LIMIT",
      "id": "61eafb449cdd4ea98a8faba809a5145f",
      "preview_id": "00000000000000000000000000000000"
    }
  ],
  "vars": {
    "OPENAI_API_KEY": "your_openai_api_key_here"
  },
  "site": {
    "bucket": "./dist",
    "include": [
      "**/*.html",
      "**/*.css",
      "**/*.js",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.gif",
      "**/*.ico",
      "**/*.xml",
      "**/*.txt",
      "**/*.json",
      "**/*.mp3",
      "**/*.mp4",
      "**/*.webm",
      "**/*.pdf"
    ],
    "exclude": [
      "node_modules/**",
      "*.md",
      "*.py",
      "tests/**",
      "__pycache__/**",
      "scripts/**",
      "requirements*.txt",
      ".env*",
      "temp_venv/**",
      ".git/**",
      ".github/**"
    ]
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
